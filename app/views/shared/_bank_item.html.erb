<li id="<%= bank_item.id %>">
    <span class="content">
       <%= bank_item.nickname %> - <%= bank_item.checking %> - <%= bank_item.routing %> - <%= bank_item.bankname %>
     
     <div class="addnew">
  <% if current_user?(bank_item.user) %>
    <%= link_to '<i class="icon-trash"></i>'.html_safe, bank_item, method:  :delete,
                                     confirm: "You sure?",
                                     title:   bank_item.nickname %>
    
  <% end %>
  </span>
</div>
</li>


